@model IEnumerable<TopGym_System.Models.Subscription>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SubscriptionName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PricePerGym)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PricePerGymAndCardio)
        </th>
        <th> Actions</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SubscriptionName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PricePerGym)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PricePerGymAndCardio)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
